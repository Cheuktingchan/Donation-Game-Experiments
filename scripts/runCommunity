#!/usr/bin/bash
for k in {1..100}
do
  echo run number ${k}
  java -classpath .:./bin:./lib/commons-cli-1.5.0/commons-cli-1.5.0.jar Simulation -g 100000 -net 3 24 0.05 -quiet -intervals 100
  java -classpath .:./bin:./lib/commons-cli-1.5.0/commons-cli-1.5.0.jar Simulation -g 100000 -net 3 11 0 -quiet -intervals 100
done